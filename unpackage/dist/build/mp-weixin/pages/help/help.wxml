<view class="content"><view id="main"><view id="questionTypeView"><block wx:for="{{question.list}}" wx:for-item="item" wx:for-index="index" wx:key="name"><view data-event-opts="{{[['tap',[['questionTypeDetail',[index]]]]]}}" class="{{['questionType',(index==question.index)?'questionTypeSelect':'']}}" bindtap="__e"><text>{{item.name}}</text></view></block></view><view id="questionView"><block wx:for="{{question.list[question.index].list}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view data-event-opts="{{[['tap',[['questionDetail',['$0'],[[['question.list.'+question.index+'.list','',index]]]]]]]}}" class="question" bindtap="__e"><text>{{item.name}}</text></view></block></view></view></view>