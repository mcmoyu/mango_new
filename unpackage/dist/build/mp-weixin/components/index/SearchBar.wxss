#searchBar{height:100rpx;display:flex;padding:0 20rpx;align-items:center;background-color:#f21724}#search{flex:1;height:60rpx;color:#828282;padding:10rpx;font-size:26rpx;position:relative;align-items:center;border-radius:50rpx;background-color:#eee}#searchIcon{top:10rpx;height:60rpx;position:absolute}#placeholder{flex:1;top:0rpx;left:0rpx;width:100%;height:100%;display:flex;position:absolute;align-items:center;justify-content:center}#searchIcons{margin-left:5rpx;margin-right:5rpx;-webkit-transform:translateY(3rpx);transform:translateY(3rpx)}.searchIcon{width:26rpx;height:26rpx;margin-left:2rpx;margin-right:2rpx;vertical-align:initial}#help{margin-left:10rpx}